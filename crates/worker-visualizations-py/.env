# NATS Configuration
NATS_URL="nats://localhost:4222"
NATS_STREAM_RETRY_ATTEMPTS="30"
NATS_STREAM_RETRY_DELAY="2.0"
NATS_BATCH_SIZE="1"
NATS_FETCH_TIMEOUT="5.0"

# Qdrant Configuration
QDRANT_URL="http://localhost:6334"

# AWS/S3 Configuration
AWS_ENDPOINT_URL="http://localhost:9000"
AWS_ACCESS_KEY_ID="test_access_key"
AWS_SECRET_ACCESS_KEY="test_secret_key"
AWS_REGION="us-east-1"
S3_BUCKET_NAME="semantic-explorer-local"
S3_FORCE_PATH_STYLE="true"
# S3 File Size Limits (in bytes)
S3_MAX_DOWNLOAD_SIZE_BYTES="104857600"  # 100MB
S3_MAX_UPLOAD_SIZE_BYTES="1073741824"   # 1GB

# Logging
LOG_LEVEL="INFO"

# Worker Configuration
WORKER_ID="py-worker-local-1"
PROCESSING_TIMEOUT_SECS="3600"
MAX_CONCURRENT_JOBS="3"

# OpenTelemetry
OTEL_EXPORTER_OTLP_ENDPOINT="http://localhost:4317"

# Prometheus Metrics (unique port per worker to avoid conflicts)
PROMETHEUS_METRICS_PORT="9093"

